<?xml version="1.0"?>
<!DOCTYPE list SYSTEM "data/listpatron.dtd">
<list>
	<info>
		<version>0.1</version>
		<title>Boeken</title>
		<author>F.Boender</author>
		<description>A list of books in my posession.</description>
		<keywords>books, authors</keywords>
	</info>
	<state>
		<window>
			<position>
				<x>100</x>
				<y>10</y>
			</position>
			<dimensions>
				<width>500</width>
				<height>300</height>
			</dimensions>
		</window>
	</state>
	<filters>
		<filter>
			<filtername>Good books</filtername>
			<rule>
				<columnname>Rating</columnname>
				<predicate>larger than</predicate>
				<value>3</value>
			</rule>
		</filter>
		<filter>
			<filtername>Unread Science Fiction books</filtername>
			<rule>
				<columnname>Genre</columnname>
				<predicate>is</predicate>
				<value>Sci-fi</value>
			</rule>
			<rule>
				<columnname>Read</columnname>
				<predicate>is</predicate>
				<value>no</value>
			</rule>
		</filter>
	</filters>
	<sorts>
		<sort>
			<sortname>Author</sortname>
			<sortcolumns>
				<columnname sortposition="1" sortdirection="desc">Author</columnname>
				<columnname sortposition="2" sortdirection="desc">Title</columnname>
			</sortcolumns>
		</sort>
		<sort>
			<sortname>Genre</sortname>
			<sortcolumns>
				<columnname sortposition="1" sortdirection="desc">Genre</columnname>
				<columnname sortposition="2" sortdirection="desc">Author</columnname>
				<columnname sortposition="3" sortdirection="desc">Series</columnname>
			</sortcolumns>
		</sort>
	</sorts>
	<reports>
		<report>
			<reportname>Unread Science Fiction books</reportname>
			<reporttype>print</reporttype>
			<filtername>Unread Science Fiction books</filtername>
			<sortname>Author</sortname>
		</report>
	</reports>
	<header>
		<columnname viewposition="2" sortposition="4" sortdirection="asc" type="string">Title</columnname>
		<columnname viewposition="1" sortposition="2" sortdirection="asc" type="string">Author</columnname>
		<columnname viewposition="3" sortposition="5" sortdirection="asc" type="string">Genre</columnname>
		<columnname viewposition="4" sortposition="6" sortdirection="asc" type="string">Language</columnname>
		<columnname viewposition="5" sortposition="7" sortdirection="asc" type="number">Rating</columnname>
		<columnname viewposition="6" sortposition="1" sortdirection="asc" type="string">Series</columnname>
		<columnname viewposition="7" sortposition="3" sortdirection="asc" type="toggle">Read</columnname>
	</header>
